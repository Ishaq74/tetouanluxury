---
import MainLayout from '../layouts/MainLayout.astro';

const lang = 'en';
---

<MainLayout title="Discover Tétouan - Local Guide" lang={lang}>
  <div class="py-20">
    <div class="container-custom">
      <h1 class="text-5xl font-serif font-bold mb-6 text-center">Discover Tétouan</h1>
      <p class="text-lg text-gray-600 mb-12 text-center max-w-2xl mx-auto">
        Your local guide to the best attractions, restaurants, and experiences in Tétouan.
      </p>
      <div class="text-center text-gray-500">
        <p>Guide content coming soon...</p>
      </div>
    </div>
  </div>
</MainLayout>
